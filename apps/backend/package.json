{
  "name": "backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf ./.turbo ./dist node_modules",
    "dev": "tsnd --respawn --transpile-only --exit-child --watch src -r tsconfig-paths/register ./src/App.ts",
    "prisma:generate": "prisma generate",
    "start": "node ./dist/App"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "api-schema": "*",
    "dotenv": "^16.0.2",
    "googleapis": "^108.0.0",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "springpress": "^1.3.3"
  },
  "devDependencies": {
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.23",
    "prisma": "^4.3.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.6.4"
  }
}
